# Note that changing the variables here will require a restart of the app server

# Variables prefix with SERVER is only exposed to the server

SERVER_HOST=0.0.0.0
SERVER_PORT=2200

# HTTP Basic Auth
SERVER_ENABLE_HTTP_BASIC_AUTH=
SERVER_HTTP_BASIC_AUTH_PATH=
SERVER_HTTP_BASIC_AUTH_USER=
SERVER_HTTP_BASIC_AUTH_PASS=

ENV_NAME=development

APP_NAME=Bedrock
API_URL=http://localhost:2300

# Sentry Error Tracking
SENTRY_DSN=

# Google Tag Manager Analytics
GTM_CONTAINER_ID=

# Google Maps and Address Lookup
# https://console.cloud.google.com/apis/library/maps-backend.googleapis.com
# https://console.cloud.google.com/apis/library/places-backend.googleapis.com
GOOGLE_API_KEY=