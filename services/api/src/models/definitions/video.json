{
  "attributes": {
    "name": {
      "type": "String",
      "required": true,
      "trim": true
    },
    "provider": {
      "type": "String",
      "required": true,
      "enum": [
        "gcloud",
        "cloudflare",
        "vimeo",
        "mux"
      ]
    },
    "cloudflareId": {
      "type": "String"
    },
    "muxAssetId": {
      "type": "String"
    },
    "vimeoUri": {
      "type": "String"
    },
    "vimeoThumbnail": {
      "type": "String"
    },
    "muxPlaybackIds": [
      {
        "id": "String",
        "policy": "String"
      }
    ],
    "upload": {
      "type": "ObjectId",
      "ref": "Upload",
      "required": true,
      "autopopulate": true
    },
    "captions": [
      {
        "kind": {
          "type": "String",
          "enum": [
            "subtitles",
            "captions",
            "metadata"
          ]
        },
        "language": {
          "type": "String",
          "enum": [
            "en",
            "fr",
            "de"
          ],
          "default": "en"
        },
        "upload": {
          "type": "ObjectId",
          "ref": "Upload",
          "autopopulate": true
        }
      }
    ],
    "transcodeJobs": [
      {
        "name": "String"
      }
    ]
  }
}
